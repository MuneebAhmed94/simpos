<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="_assets_primary_variables" inherit_id="polaco._assets_primary_variables">
        <xpath expr="//link[@href='/polaco/static/src/scss/primary_variables.scss']" position="after">
            <link rel="stylesheet" type="text/scss" href="/chateraise/static/src/scss/primary_variables.scss"/>
        </xpath>
    </template>

    <template id="assets" name="point_of_sale assets" inherit_id="simpos.assets">
        <xpath expr="//link[@href='/simpos/static/src/css/simpos_variables.scss']" position="after">
            <link rel="stylesheet" type="text/scss" href="/chateraise/static/src/scss/simpos_variables.scss"/>
        </xpath>
        <xpath expr="//link[@href='/simpos/static/src/css/simpos.scss']" position="after">
            <link rel="stylesheet" type="text/scss" href="/chateraise/static/src/scss/simpos_chateraise.scss"/>
        </xpath>
        <xpath expr="//script[@src='/simpos/static/src/js/models.js']" position="after">
            <script type="text/javascript" src="/chateraise/static/src/js/models.js"></script>
        </xpath>
    </template>
</odoo>