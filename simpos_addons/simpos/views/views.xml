<odoo>
  <data>
    <template id="index" inherit_id="point_of_sale.index">
        <xpath expr="//title" position="replace">
            <title>Simpos</title>
        </xpath>
        <xpath expr="//meta[@name='viewport']" position="replace">
            <meta content="width=device-width, initial-scale=1" name="viewport" />
        </xpath>
        <xpath expr="//link[@href='/web/static/src/img/favicon.ico']" position="replace">
            <link rel="shortcut icon" href="/simpos/static/src/img/favicon.ico" type="image/x-icon"/>
        </xpath>
        
    </template>
    <template id="assets" name="point_of_sale assets" inherit_id="point_of_sale.assets">
        <xpath expr="//link[@href='/point_of_sale/static/src/css/pos.css']" position="replace">
            <link rel="stylesheet" type="text/scss" href="/simpos/static/src/css/reboot.css" />
            <link rel="stylesheet" type="text/scss" href="/simpos/static/src/css/grid.css" />
            <link rel="stylesheet" type="text/scss" href="/simpos/static/src/css/utilities.css" />
            <link rel="stylesheet" type="text/scss" href="/simpos/static/src/css/original.scss" />
            <link rel="stylesheet" type="text/scss" href="/simpos/static/src/css/simpos_variables.scss" />
            <link rel="stylesheet" type="text/scss" href="/simpos/static/src/css/simpos.scss" />
        </xpath>
        <xpath expr="//script[@src='/point_of_sale/static/src/js/screens.js']" position="after">
            <script type="text/javascript" src="/simpos/static/src/js/screens.js"></script>
        </xpath>
        <xpath expr="//script[@src='/point_of_sale/static/src/js/gui.js']" position="after">
            <script type="text/javascript" src="/simpos/static/src/js/gui.js"></script>
        </xpath>
        <xpath expr="//script[@src='/point_of_sale/static/src/js/models.js']" position="after">
            <script type="text/javascript" src="/simpos/static/src/js/models.js"></script>
        </xpath>
    </template>
  </data>
</odoo>